.industry_content{
    background-color: #161616;
        position: absolute;
        left: 0;
        top: 0;
        bottom: 0;
        right: 0;

}
.header {
    width: 95%;
    height: 50rpx;
    /* background: red; */
    margin-left: 4%;
    margin-top: 15rpx;
    color: #ecb167;
