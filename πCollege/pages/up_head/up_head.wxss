/* pages/up_head/up_head.wxss */

.insert_content {
  width: 100%;
  height: auto;
  background-color: #161616;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
}
.headBox{
  width: 70%;
  height: 50rpx;
  background-color:orange;
  border-radius:16rpx;
  margin:0 auto;
  text-align: center;
  line-height: 50rpx;
  margin-top: 20rpx;

}
.insert {
  width: 700rpx;
  margin-left: 5%;
  height: auto;
  background: cadetblue;
  margin-top: 30rpx;
}
.image_box {
  width: 100rpx;
  height: 100rpx;
  float: left;
  background: #ccc;
  border: dashed 1rpx #999;
  border-radius: 16rpx;
}
.image_box image {
  width:100%;
  height: 100%;
  float: left;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
}
.dataImg{
   width: 100rpx;
  height: 100rpx;
  float: left;
  background: #fff;
  border: dashed 1rpx #999;
  border-radius: 16rpx;
  margin:0 10rpx 0 10rpx;
}
.clear {
  clear: both;
}
